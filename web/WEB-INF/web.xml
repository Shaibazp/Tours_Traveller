<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
    version="4.0">
    <servlet>
        <servlet-name>savePlaces</servlet-name>
        <servlet-class>savePlaces</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveHotels</servlet-name>
        <servlet-class>saveHotels</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>savereview</servlet-name>
        <servlet-class>savereview</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>savePlaces</servlet-name>
        <url-pattern>/uploadServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveHotels</servlet-name>
        <url-pattern>/hotelUrl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>savereview</servlet-name>
        <url-pattern>/addreview</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Home.jsp</welcome-file>
    </welcome-file-list>
</web-app>
